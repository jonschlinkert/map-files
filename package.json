{
  "name": "map-files",
  "description": "Return an object for a glob of files. Pass a `rename` function for the keys, or a `parse` function for the content, allowing it to be used for readable or require-able files.",
  "version": "0.8.2",
  "homepage": "https://github.com/jonschlinkert/map-files",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/map-files",
  "bugs": {
    "url": "https://github.com/jonschlinkert/map-files/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "isobject": "^2.0.0",
    "lazy-cache": "^1.0.4",
    "matched": "^0.4.1",
    "vinyl": "^1.1.1"
  },
  "devDependencies": {
    "extend-shallow": "^2.0.1",
    "gulp-format-md": "^0.1.8",
    "js-yaml": "^3.6.0",
    "mocha": "^2.4.5",
    "relative": "^3.0.2",
    "should": "^8.3.1"
  },
  "keywords": [
    "dir",
    "directories",
    "directory",
    "dirs",
    "file",
    "filepath",
    "filepaths",
    "files",
    "hash",
    "list",
    "map",
    "mapping",
    "name",
    "object",
    "path",
    "paths"
  ],
  "verb": {
    "related": {
      "list": [
        "export-files",
        "file-reader",
        "filter-files",
        "micromatch"
      ]
    },
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "verb",
      "vinyl"
    ]
  }
}
